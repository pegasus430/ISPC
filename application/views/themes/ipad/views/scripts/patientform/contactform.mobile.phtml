					$('.'+cname).each(function(){
						var text="";
						$(this).find('input').each(function(){

							if($(this).attr('type')=='checkbox'){
								if($(this).attr('checked') == "checked") text=text+'X';
								} 
							else {
								text = text + $(this).val();			
								}
							});
						if (text=="") emptyrows++;
						});
					if(emptyrows==0){
						var newele = $('.'+cname+':last').clone();

						$('.'+cname+':last').after(newele);
						$(newele).find('input').each(function(){
							if($(this).attr('type')=='checkbox'){
								$(this).removeAttr('checked');
								} 
							else {
								$(this).val('');			
